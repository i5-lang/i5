#!/usr/bin/env i5

fn main() {
	fizzbuzz()
}

fn fizzbuzz() {
	fn for(i end) {
		if i <= end {
			result = ""
			if i % 3 == 0 {
				result := "Fizz"
			}
			if i % 5 == 0 {
				result := "Buzz"
			}
			if result != "" {
				$print(result)
			} else {
				$print(i)
			}
			i += 1
			for(i, end)
		}
	}
	for(1, 100)
}
