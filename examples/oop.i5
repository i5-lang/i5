#!/usr/bin/env i5

fn main() {
	object = newObj()
	object.setName("My Name")
	object.setAge(0)
	$print("name: " : object.getName())
	# output: "name: My Name"
	$print("age: " : object.getAge())
	# output: "age: 0"
}

fn newObj() {
	map = $map.new()
	map.name = ""
	map.age = 0
	map.getName = fn() {
		return map.name
	}
	map.getAge = fn() {
		return map.age
	}
	map.setName = fn(name) {
		map.name = name
	}
	map.setAge = fn(age) {
		map.age = age
	}
	return map
}
